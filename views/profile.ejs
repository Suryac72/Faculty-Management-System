<%- include('./layouts/header') %>

<h1><span class="goback" onclick="window.history.back()">←</span> Profile</h1>

<div class="profile">
  <div>
    <p class="title">Id</p>
    <p class="subtitle"><%= person.id %></p>
  </div>
  <div>
    <p class="title">Name</p>
    <p class="subtitle"><%= person.name %></p>
  </div>
  <div>
    <p class="title">Email/username</p>
    <p class="subtitle"><%= person.email %></p>
  </div>
  <div>
    <p class="title">Role</p>
    <p class="subtitle"><%= person.role %></p>
  </div>
  <div>
    <p class="title">Contact Number</p>
    <pre class="subtitle"><%= person.contactNumber %></pre>
  </div>
  <div>
    <p class="title">Address</p>
    <pre class="subtitle"><%= person.address %></pre>
  </div>
</div>
<%- include('./layouts/footer') %>